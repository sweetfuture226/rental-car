{
  "name": "crm",
  "version": "1.0.0",
  "description": "A crm app",
  "author": "Kelvin Esegbona, Jude Chinoso",
  "license": "ISC",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "tsc && node built/index && node bree.js",
    "clean:test:db": "rm -rf localdb",
    "seedPermissionAndRoles": "node ./src/utils/runRoleSeed",
    "seedFormData": "node ./src/utils/runFormElementSeed",
    "test": "npm run clean:test:db",
    "test:watch": "npm run clean:test:db",
    "dev": "ts-node-dev src/index",
    "build": "tsc && npm run migrate:all",
    "bree": "nodemon bree.js",
    "migrate:undo:all": "npm run migrate:shopit:undo:all",
    "migrate:undo": "npm run migrate:crm:undo",
    "seed:crm": "cross-env db_name=crm sequelize --options-path ./.sequelizerc-crm db:seed:all",
    "seed:idmb": "cross-env db_name=idemandbeauti sequelize --options-path ./.sequelizerc-idemandbeauti db:seed:all",
    "crm:model:generate": "sequelize --options-path ./.sequelizerc-crm model:generate",
    "shopit:model:generate": "sequelize --options-path ./.sequelizerc-shopit model:generate",
    "migrate:crm": "cross-env db_name=crm sequelize --options-path ./.sequelizerc-crm db:migrate",
    "migrate:shopit": "cross-env db_name=shopit sequelize --options-path ./.sequelizerc-shopit db:migrate",
    "migrate:shopit:undo": "cross-env db_name=shopit sequelize --options-path ./.sequelizerc-shopit db:migrate:undo",
    "migrate:idemandbeauti": "cross-env db_name=idemandbeauti sequelize --options-path ./.sequelizerc-idemandbeauti db:migrate",
    "migrate:shopit:undo:all": "cross-env db_name=shopit sequelize --options-path ./.sequelizerc-shopit db:migrate:undo:all",
    "migrate:all": "npm run migrate:crm && npm run migrate:shopit && npm run migrate:idemandbeauti",
    "get:secrets": "node built/app"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@babel/register": "^7.16.9",
    "@ladjs/graceful": "^1.1.5",
    "@sendgrid/mail": "^7.6.2",
    "aws-sdk": "^2.1092.0",
    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "bree": "^8.0.2",
    "busboy": "^1.4.0",
    "cabin": "^9.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "email-templates": "^8.0.8",
    "express": "^4.17.2",
    "express-form-data": "^2.0.17",
    "jade": "^1.11.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "os": "^0.1.2",
    "pg": "^8.7.1",
    "query-string": "^7.1.1",
    "rollbar": "^2.24.0",
    "sequelize": "^6.13.0",
    "signale": "^1.4.0",
    "stripe": "^8.203.0",
    "twilio": "^3.75.0",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.12"
  }
}
